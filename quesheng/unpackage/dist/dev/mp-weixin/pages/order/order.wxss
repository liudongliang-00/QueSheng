.huous_hao{
		font-size: 28rpx;
		color: #333333;
		line-height: 46rpx;
		display: -webkit-box;
		display: -webkit-flex;
		display: flex;
}
.huous_hao text:last-child{
		display: block;
		width: 320rpx;
		margin-left: 30rpx;
		overflow:hidden; /*超出的文本隐藏 */
		text-overflow:ellipsis; /*溢出用省略号显示 */
		white-space:nowrap; /*溢出不换行 */
}
.huous_title{
		color: #485AEA;
}
