<view><view class="head_img"><view class="header"><view class="head_area"><view class="uni-list-cell-db"><picker range="{{area_info}}" range-key="name" data-event-opts="{{[['change',[['bindPickerChange',['$event']]]]]}}" bindchange="__e"><view class="uni-input sel-input">{{area_name}}</view></picker><text class="iconfont icon-xia ico_xia"></text></view></view><view class="head_input"><text data-event-opts="{{[['tap',[['postIndex',['$event']]]]]}}" class="iconfont icon-sousuo sou" bindtap="__e"></text><input type="text" placeholder="请输入门店名称" confirm-type="search" data-event-opts="{{[['input',[['sousuos',['$event']]]]]}}" value="{{input_value}}" bindinput="__e"/></view></view></view><view class="particulars"><block wx:for="{{indexInfo}}" wx:for-item="item" wx:for-index="__i0__" wx:key="id"><view data-event-opts="{{[['tap',[['particulars',['$0','$1','$2'],[[['indexInfo','id',item.id,'id']],[['indexInfo','id',item.id,'x']],[['indexInfo','id',item.id,'y']]]]]]]}}" class="particulars_content" bindtap="__e"><view class="content_img"><image src="{{item.door_photos}}" mode="aspectFill" lazy-load="true"></image><view class="content_house">{{item.ishave==1?"有房":"无房"}}</view></view><view class="content"><text class="content_title">{{item.shopname}}</text><view class="content_middle"><text class="middle1">￥</text><text class="middle2">{{item.mini_price}}</text><text class="middle1 middle3">/小时</text>起</view><view class="content_footer"><text class="footer1">{{item.address}}</text><text>{{"|  "+item.distance+"km"}}</text></view></view></view></block></view></view>